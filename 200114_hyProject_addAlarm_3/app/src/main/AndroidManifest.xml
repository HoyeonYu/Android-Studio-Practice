<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a200114_hyproject_addAlarm_3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.example.a200113_hyproject.LoginActivity"
            android:label="hyProject"
            android:theme="@style/AppTheme.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.a200113_hyproject.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity
            android:name="com.example.a200113_hyproject.Alarm_MainActivity">
        </activity>

        <receiver android:name="com.example.a200113_hyproject.AlarmReceiver"/>

        <service
            android:name="com.example.a200113_hyproject.RingtonePlayService"
            android:enabled="true"></service>


    </application>

</manifest>